{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.main.tsbuildinfo",
    "outDir": "./dist/main",
    "rootDir": "./src/main"
  },
  "include": [
    "src/main/**/*.ts",
    "src/shared/**/*.ts",
    "src/env.d.ts"
  ],
  "exclude": [
    "src/main/**/*.test.ts"
  ]
}